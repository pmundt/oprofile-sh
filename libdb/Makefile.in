include ../make.common

HEADERS=db.h
 
SOURCES=db-manage.c db-insert.c db-travel.c db-debug.c

OBJECTS=db-manage.o db-insert.o db-travel.o db-debug.o

TARGET=libdb.a

all: $(TARGET)
install:
uninstall:
clean:
	rm -rf .deps $(OBJECTS) $(TARGET) 

$(TARGET): $(OBJECTS)
	$(AR) cr $@ $^

db-test: db-test.c $(TARGET)
	$(CC) -o $@ db-test.c $(TARGET)
 
include ../make.deps
